extends layout

block content

    div(class="stereo")
        div(class='gameList js-gameListView') game list
        div(class='playerList js-playerListView')
            ul
                each user in userList
                    li= user

    div(class="stereo")
        button(class="btn") create a game
        button(class="btn") join a game

    div(class='js-messageView') empty
    button(class='js-sendBtn') home view
